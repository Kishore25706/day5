package Day5;

public class string {

	public static void main(String[] args) {
         String str="Aakash";
         String str1="Aakash";
         String str2=new String("Aakash");
         String str3=new String("green");
         System.out.println(str);
         System.out.println(str==str1);
         System.out.println(str.equals(str3));
         System.out.println(str.equalsIgnoreCase(str3));
         System.out.println(str.length());
         System.out.println(str.trim().length());
         System.out.println(str.length());
         String Word[]=str.split(" ");
         for(String var:Word) {
        	 System.out.println(var);
         }
         System.out.println(str2.substring(1,4));
         System.out.println(str2.contains("kishore"));
         System.out.println(str2.endsWith("cs"));
         System.out.println(str2.charAt(2));
         System.out.println(str2.concat("star"));
	}

}
